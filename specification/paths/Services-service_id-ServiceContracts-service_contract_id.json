{
  "get": {
    "tags": [
      "Services"
    ],
    "summary": "Get a service contract for a service by id.",
    "description": "This endpoint retrieves a service contract for the supplied service.",
    "parameters": [
      {
        "in": "path",
        "name": "service_id",
        "type": "string",
        "required": true,
        "description": "Id of the service."
      },
      {
        "in": "path",
        "name": "service_contract_id",
        "type": "string",
        "required": true,
        "description": "Id of the service contract."
      },
      {
        "in": "query",
        "name": "include",
        "type": "string",
        "description": "Comma seperated list of relationship names you want to include the data of.<br> The relationships that can be included are carrier-contract, service-groups, service-insurances and service-options."
      }
    ],
    "responses": {
      "200": {
        "description": "Retrieved the service contract",
        "schema": {
          "type": "object",
          "required": [
            "data",
            "links"
          ],
          "properties": {
            "data": {
              "$ref": "#/definitions/ServiceContract"
            },
            "links": {
              "type": "object",
              "required": [
                "self"
              ],
              "properties": {
                "self": {
                  "type": "string",
                  "example": "$base_url/services/c9ce29a4-6325-11e7-907b-a6006ad3dba0/contracts/c9ce29a4-6325-11e7-907b-a6006ad3dba0"
                }
              }
            }
          }
        }
      }
    }
  }
}
