{
  "get": {
    "tags": [
      "Carriers"
    ],
    "summary": "Get all the pickup and dropoff locations.",
    "description": "This endpoint retrieves pickup and dropoff locations.",
    "parameters": [
      {
        "in": "path",
        "name": "carrier_id",
        "type": "string",
        "required": true,
        "description": "Id of the carrier."
      },
      {
        "in": "path",
        "name": "country_code",
        "required": true,
        "type": "string",
        "description": "The country code."
      },
      {
        "in": "path",
        "name": "postal_code",
        "required": true,
        "type": "string",
        "description": "The postal code."
      },
      {
        "in": "path",
        "name": "street",
        "required": true,
        "type": "string",
        "description": "The street."
      },
      {
        "in": "path",
        "name": "street_number",
        "required": true,
        "type": "string",
        "description": "The street number."
      },
      {
        "in": "query",
        "name": "filter[carriers]",
        "type": "string",
        "description": "The carrier id(s)."
      }
    ],
    "responses": {
      "200": {
        "description": "Retrieved the location(s).",
        "schema": {
          "type": "object",
          "required": [
            "data",
            "meta",
            "links"
          ],
          "additionalProperties": false,
          "properties": {
            "data": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/PickupDropoffLocation"
              }
            },
            "meta": {
              "type": "object",
              "required": [
                "total_pages"
              ],
              "properties": {
                "total_pages": {
                  "type": "number",
                  "example": 13
                }
              }
            },
            "links": {
              "type": "object",
              "required": [
                "self",
                "first",
                "last"
              ],
              "properties": {
                "self": {
                  "type": "string",
                  "example": "$base_url/pickup-dropoff-locations/NL/1234AB?page[number]=3&page[size]=30"
                },
                "first": {
                  "type": "string",
                  "example": "$base_url/pickup-dropoff-locations/NL/1234AB?page[number]=1&page[size]=30"
                },
                "prev": {
                  "type": "string",
                  "example": "$base_url/pickup-dropoff-locations/NL/1234AB?page[number]=2&page[size]=30"
                },
                "next": {
                  "type": "string",
                  "example": "$base_url/pickup-dropoff-locations/NL/1234AB?page[number]=4&page[size]=30"
                },
                "last": {
                  "type": "string",
                  "example": "$base_url/pickup-dropoff-locations/NL/1234AB?page[number]=13&page[size]=30"
                }
              }
            }
          }
        }
      }
    }
  }
}
