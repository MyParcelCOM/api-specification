{
  "get": {
    "tags": [
      "Services"
    ],
    "summary": "Get all the service options for a service contract.",
    "description": "This endpoint retrieves service options for the supplied service contract.",
    "parameters": [
      {
        "in": "path",
        "name": "service_id",
        "type": "string",
        "required": true,
        "description": "Id of the service."
      },
      {
        "in": "path",
        "name": "service_contract_id",
        "type": "string",
        "required": true,
        "description": "Id of the service contract."
      }
    ],
    "responses": {
      "200": {
        "description": "Retrieved the service option(s).",
        "schema": {
          "type": "object",
          "required": [
            "data",
            "meta",
            "links"
          ],
          "additionalProperties": false,
          "properties": {
            "data": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/ServiceOption"
              }
            },
            "meta": {
              "$ref": "../includes/pagination-meta.json"
            },
            "links": {
              "type": "object",
              "required": [
                "self",
                "first",
                "last"
              ],
              "properties": {
                "self": {
                  "type": "string",
                  "example": "$base_url/services/c9ce29a4-6325-11e7-907b-a6006ad3dba0/contracts/c9ce29a4-6325-11e7-907b-a6006ad3dba0/options?page[number]=3&page[size]=30"
                },
                "first": {
                  "type": "string",
                  "example": "$base_url/services/c9ce29a4-6325-11e7-907b-a6006ad3dba0/contracts/c9ce29a4-6325-11e7-907b-a6006ad3dba0/options?page[number]=1&page[size]=30"
                },
                "prev": {
                  "type": "string",
                  "example": "$base_url/services/c9ce29a4-6325-11e7-907b-a6006ad3dba0/contracts/c9ce29a4-6325-11e7-907b-a6006ad3dba0/options?page[number]=2&page[size]=30"
                },
                "next": {
                  "type": "string",
                  "example": "$base_url/services/c9ce29a4-6325-11e7-907b-a6006ad3dba0/contracts/c9ce29a4-6325-11e7-907b-a6006ad3dba0/options?page[number]=4&page[size]=30"
                },
                "last": {
                  "type": "string",
                  "example": "$base_url/services/c9ce29a4-6325-11e7-907b-a6006ad3dba0/contracts/c9ce29a4-6325-11e7-907b-a6006ad3dba0/options?page[number]=13&page[size]=30"
                }
              }
            }
          }
        }
      }
    }
  }
}