{
  "get": {
    "tags": [
      "Carriers"
    ],
    "summary": "Get all the services from a carrier.",
    "description": "This endpoint retrieves services for the supplied carriers.",
    "parameters": [
      {
        "in": "path",
        "name": "carrier_id",
        "type": "string",
        "required": true,
        "description": "Id or comma separated list of ids of the carrier(s)."
      },
      {
        "in": "query",
        "name": "filter[from][country_code]",
        "type": "string",
        "description": "The from country code."
      },
      {
        "in": "query",
        "name": "filter[from][region_code]",
        "type": "string",
        "description": "The from region code."
      },
      {
        "in": "query",
        "name": "filter[to][country_code]",
        "type": "string",
        "description": "The to country code."
      },
      {
        "in": "query",
        "name": "filter[to][region_code]",
        "type": "string",
        "description": "The to region code."
      }
    ],
    "responses": {
      "200": {
        "description": "Retrieved the service(s).",
        "schema": {
          "type": "object",
          "required": [
            "data",
            "meta",
            "links"
          ],
          "additionalProperties": false,
          "properties": {
            "data": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Service"
              }
            },
            "meta": {
              "type": "object",
              "required": [
                "total_pages"
              ],
              "properties": {
                "total_pages": {
                  "type": "number",
                  "example": 13
                }
              }
            },
            "links": {
              "type": "object",
              "required": [
                "self",
                "first",
                "last"
              ],
              "properties": {
                "self": {
                  "type": "string",
                  "example": "$base_url/carriers/c9ce29a4-6325-11e7-907b-a6006ad3dba0/services?page[number]=3&page[size]=30"
                },
                "first": {
                  "type": "string",
                  "example": "$base_url/carriers/c9ce29a4-6325-11e7-907b-a6006ad3dba0/services?page[number]=1&page[size]=30"
                },
                "prev": {
                  "type": "string",
                  "example": "$base_url/carriers/c9ce29a4-6325-11e7-907b-a6006ad3dba0/services?page[number]=2&page[size]=30"
                },
                "next": {
                  "type": "string",
                  "example": "$base_url/carriers/c9ce29a4-6325-11e7-907b-a6006ad3dba0/services?page[number]=4&page[size]=30"
                },
                "last": {
                  "type": "string",
                  "example": "$base_url/carriers/c9ce29a4-6325-11e7-907b-a6006ad3dba0/services?page[number]=13&page[size]=30"
                }
              }
            }
          }
        }
      }
    }
  }
}
