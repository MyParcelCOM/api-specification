{
  "get": {
    "tags": [
      "Carriers"
    ],
    "summary": "Get a contract for a carrier by id.",
    "description": "This endpoint retrieves a contract for the supplied carrier.",
    "parameters": [
      {
        "in": "path",
        "name": "carrier_id",
        "type": "string",
        "required": true,
        "description": "Id of the carrier."
      },
      {
        "in": "path",
        "name": "carrier_contract_id",
        "type": "string",
        "required": true,
        "description": "Id of the contract."
      }
    ],
    "responses": {
      "200": {
        "description": "Retrieved the contract for the carrier.",
        "schema": {
          "type": "object",
          "required": [
            "data",
            "links"
          ],
          "additionalProperties": false,
          "properties": {
            "data": {
              "$ref": "#/definitions/CarrierContract"
            },
            "links": {
              "type": "object",
              "required": [
                "self"
              ],
              "properties": {
                "self": {
                  "type": "string",
                  "example": "$base_url/carriers/c9ce29a4-6325-11e7-907b-a6006ad3dba0/contracts/c9ce29a4-6325-11e7-907b-a6006ad3dba0"
                }
              }
            }
          }
        }
      }
    }
  }
}
