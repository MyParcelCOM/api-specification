{
  "get": {
    "tags": [
      "Services"
    ],
    "summary": "Get all the services.",
    "description": "This endpoint retrieves services.",
    "parameters": [
      {
        "in": "query",
        "name": "filter[package_type]",
        "type": "string",
        "description": "Comma separated string of package types to filter on."
      },
      {
        "in": "query",
        "name": "filter[carrier]",
        "type": "string",
        "description": "Comma separated string of carrier ids to filter on."
      },
      {
        "in": "query",
        "name": "filter[region_from]",
        "type": "string",
        "description": "Comma separated string of region ids to filter on."
      },
      {
        "in": "query",
        "name": "filter[region_to]",
        "type": "string",
        "description": "Comma separated string of region ids to filter on."
      }
    ],
    "responses": {
      "200": {
        "description": "Retrieved the service(s).",
        "schema": {
          "type": "object",
          "required": [
            "data",
            "meta",
            "links"
          ],
          "additionalProperties": false,
          "properties": {
            "data": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Service"
              }
            },
            "meta": {
              "$ref": "../includes/pagination-meta.json"
            },
            "links": {
              "type": "object",
              "required": [
                "self",
                "first",
                "last"
              ],
              "properties": {
                "self": {
                  "type": "string",
                  "example": "$base_url/services?page[number]=3&page[size]=30"
                },
                "first": {
                  "type": "string",
                  "example": "$base_url/services?page[number]=1&page[size]=30"
                },
                "prev": {
                  "type": "string",
                  "example": "$base_url/services?page[number]=2&page[size]=30"
                },
                "next": {
                  "type": "string",
                  "example": "$base_url/services?page[number]=4&page[size]=30"
                },
                "last": {
                  "type": "string",
                  "example": "$base_url/services?page[number]=13&page[size]=30"
                }
              }
            }
          }
        }
      }
    }
  }
}
