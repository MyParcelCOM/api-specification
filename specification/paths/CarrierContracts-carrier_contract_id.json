{
  "get": {
    "tags": [
      "CarrierContracts"
    ],
    "summary": "Get a carrier contract by id.",
    "description": "This endpoint retrieves a carrier contract for the supplied id.",
    "parameters": [
      {
        "$ref": "#/components/parameters/path-carrier_contract_id"
      },
      {
        "name": "include",
        "in": "query",
        "description": "Comma separated string of the relationship names you want to include the data of.<br>The relationships that can be included are:<ul><li>carrier</li><li>owner</li></ul>",
        "schema": {
          "type": "string"
        }
      }
    ],
    "responses": {
      "200": {
        "description": "Retrieved the carrier contract.",
        "content": {
          "application/vnd.api+json": {
            "schema": {
              "type": "object",
              "required": [
                "data"
              ],
              "additionalProperties": false,
              "properties": {
                "data": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/CarrierContract"
                    },
                    {
                      "properties": {
                        "attributes": {
                          "type": "object",
                          "required": [
                            "active",
                            "status",
                            "currency",
                            "name"
                          ]
                        }
                      }
                    }
                  ]
                }
              }
            }
          }
        }
      }
    }
  },
  "patch": {
    "tags": [
      "CarrierContracts"
    ],
    "summary": "Update an carrier contract by id.",
    "description": "This endpoint updates the carrier contract with the supplied id.",
    "parameters": [
      {
        "$ref": "#/components/parameters/path-carrier_contract_id"
      }
    ],
    "requestBody": {
      "description": "The carrier contract object with attributes to be updated.",
      "required": true,
      "content": {
        "application/vnd.api+json": {
          "schema": {
            "type": "object",
            "required": [
              "data"
            ],
            "additionalProperties": false,
            "properties": {
              "data": {
                "$ref": "#/components/schemas/PatchCarrierContract"
              }
            }
          }
        }
      }
    },
    "responses": {
      "200": {
        "description": "The carrier contract is updated.",
        "content": {
          "application/vnd.api+json": {
            "schema": {
              "type": "object",
              "required": [
                "data"
              ],
              "additionalProperties": false,
              "properties": {
                "data": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/CarrierContract"
                    },
                    {
                      "properties": {
                        "attributes": {
                          "type": "object",
                          "required": [
                            "active",
                            "status",
                            "currency",
                            "name"
                          ]
                        }
                      }
                    }
                  ]
                }
              }
            }
          }
        }
      }
    }
  },
  "delete": {
    "tags": [
      "CarrierContracts"
    ],
    "summary": "Delete a carrier contract by id.",
    "description": "This endpoint deletes the carrier contract with the supplied id.",
    "parameters": [
      {
        "$ref": "#/components/parameters/path-carrier_contract_id"
      }
    ],
    "responses": {
      "202": {
        "description": "The carrier contract will be deleted."
      },
      "204": {
        "description": "The carrier contract is deleted."
      }
    }
  }
}
