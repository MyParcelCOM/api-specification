{
  "get": {
    "tags": [
      "Shops"
    ],
    "summary": "Get webhooks from a shop by id.",
    "description": "This endpoint retrieves the webhook with supplied id. A comma separated list of ids can be supplied to retrieves multiple webhooks.",
    "parameters": [
      {
        "in": "path",
        "name": "shop_id",
        "type": "string",
        "required": true,
        "description": "Id of the shop."
      },
      {
        "in": "path",
        "name": "webhook_subscription_id",
        "type": "string",
        "required": true,
        "description": "Id or comma separated list of id's of the webhook subscription(s)."
      }
    ],
    "responses": {
      "200": {
        "description": "Retrieve the webhook subscription(s).",
        "schema": {
          "type": "object",
          "properties": {
            "data": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/WebhookSubscription"
              }
            },
            "meta": {
              "type": "object",
              "properties": {
                "total-pages": {
                  "type": "number",
                  "example": 13
                }
              }
            },
            "links": {
              "type": "object",
              "properties": {
                "self": {
                  "type": "string",
                  "example": "$base_url/shops/c9ce29a4-6325-11e7-907b-a6006ad3dba0/webhook-subscriptions/c9ce29a4-6325-11e7-907b-a6006ad3dba0?page[number]=3&page[size]=30"
                },
                "first": {
                  "type": "string",
                  "example": "$base_url/shops/c9ce29a4-6325-11e7-907b-a6006ad3dba0/webhook-subscriptions/c9ce29a4-6325-11e7-907b-a6006ad3dba0?page[number]=1&page[size]=30"
                },
                "prev": {
                  "type": "string",
                  "example": "$base_url/shops/c9ce29a4-6325-11e7-907b-a6006ad3dba0/webhook-subscriptions/c9ce29a4-6325-11e7-907b-a6006ad3dba0?page[number]=2&page[size]=30"
                },
                "next": {
                  "type": "string",
                  "example": "$base_url/shops/c9ce29a4-6325-11e7-907b-a6006ad3dba0/webhook-subscriptions/c9ce29a4-6325-11e7-907b-a6006ad3dba0?page[number]=4&page[size]=30"
                },
                "last": {
                  "type": "string",
                  "example": "$base_url/shops/c9ce29a4-6325-11e7-907b-a6006ad3dba0/webhook-subscriptions/c9ce29a4-6325-11e7-907b-a6006ad3dba0?page[number]=13&page[size]=30"
                }
              }
            }
          }
        }
      }
    }
  },
  "delete": {
    "tags": [
      "Shops"
    ],
    "summary": "Delete webhooks by id.",
    "description": "This endpoint deletes the webhook with supplied id. A comma separated list of ids can be supplied to delete multiple webhooks.",
    "parameters": [
      {
        "in": "path",
        "name": "shop_id",
        "type": "string",
        "required": true,
        "description": "Id of the shop."
      },
      {
        "in": "path",
        "name": "webhook_subscription_id",
        "type": "string",
        "required": true,
        "description": "Id or comma separated list of id's of the webhook subscription(s)."
      }
    ],
    "responses": {
      "202": {
        "description": "The webhook subscription(s) will be deleted."
      },
      "204": {
        "description": "The webhook subscription(s) are deleted."
      }
    }
  }
}
