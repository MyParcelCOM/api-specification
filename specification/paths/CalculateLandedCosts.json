{
  "post": {
    "tags": [
      "RPC"
    ],
    "security": [
      {
        "OAuth2": [
          "landed_cost"
        ]
      }
    ],
    "summary": "Get landed costs calculation.",
    "description": "This endpoint returns the landed costs for the supplied products.",
    "requestBody": {
      "description": "The shipment containing the products.",
      "required": true,
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/Shipment"
          }
        }
      }
    },
    "responses": {
      "200": {
        "description": "Landed costs have been returned.",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "required": [
                "data"
              ],
              "properties": {
                "data": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/Shipment"
                    }
                  ]
                }
              }
            }
          }
        }
      }
    }
  }
}
