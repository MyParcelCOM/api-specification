{
  "get": {
    "tags": [
      "Services"
    ],
    "security": [{
      "OAuth2": [
        "shipments.show",
        "shipments.manage"
      ]
    }],
    "summary": "Get all the service contracts for a service.",
    "description": "This endpoint retrieves service contracts for the supplied service.",
    "parameters": [
      {
        "$ref": "#/components/parameters/path-service_id"
      },
      {
        "name": "include",
        "in": "query",
        "description": "Comma separated string of relationship names you want to include the data of.<br>The relationships that can be included are:<ul><li>`contract`</li><li>`service`</li><li>`service_groups`</li><li>`service_option_prices`</li><li>`service_option_prices.service_option`</li></ul>",
        "schema": {
          "type": "string"
        }
      }
    ],
    "responses": {
      "200": {
        "description": "Retrieved the service contract(s).",
        "content": {
          "application/vnd.api+json": {
            "schema": {
              "type": "object",
              "required": [
                "data",
                "meta",
                "links"
              ],
              "properties": {
                "data": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/ServiceContract"
                  }
                },
                "meta": {
                  "$ref": "#/components/schemas/PaginationMeta"
                },
                "links": {
                  "type": "object",
                  "required": [
                    "self",
                    "first",
                    "last"
                  ],
                  "additionalProperties": false,
                  "properties": {
                    "self": {
                      "type": "string",
                      "example": "$API_HOST/services/175a235f-aff5-4e44-87b5-3657b75c1deb/contracts?page[number]=3&page[size]=30"
                    },
                    "first": {
                      "type": "string",
                      "example": "$API_HOST/services/175a235f-aff5-4e44-87b5-3657b75c1deb/contracts?page[number]=1&page[size]=30"
                    },
                    "prev": {
                      "type": "string",
                      "example": "$API_HOST/services/175a235f-aff5-4e44-87b5-3657b75c1deb/contracts?page[number]=2&page[size]=30"
                    },
                    "next": {
                      "type": "string",
                      "example": "$API_HOST/services/175a235f-aff5-4e44-87b5-3657b75c1deb/contracts?page[number]=4&page[size]=30"
                    },
                    "last": {
                      "type": "string",
                      "example": "$API_HOST/services/175a235f-aff5-4e44-87b5-3657b75c1deb/contracts?page[number]=13&page[size]=30"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
