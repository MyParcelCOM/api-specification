{
  "post": {
    "tags": [
      "RPC"
    ],
    "security": [
      {
        "OAuth2": [
          "shipments.manage"
        ]
      }
    ],
    "summary": "Track external shipment.",
    "description": "This endpoint is used to track externally registered shipments to provide tracking information.",
    "requestBody": {
      "description": "The shipment that should be tracked.",
      "required": true,
      "content": {
        "application/vnd.api+json": {
          "schema": {
            "type": "object",
            "required": [
              "data"
            ],
            "properties": {
              "data": {
                "allOf": [
                  {
                    "$ref": "#/components/schemas/Shipment"
                  },
                  {
                    "required": [
                      "attributes",
                      "relationships"
                    ],
                    "properties": {
                      "attributes": {
                        "required": [
                          "tracking_code"
                        ],
                        "properties": {
                          "tracking_code": {
                            "allOf": [
                              {
                                "$ref": "#/components/schemas/TrackingCode"
                              },
                              {
                                "readonly": false
                              }
                            ]
                          }
                        }
                      },
                      "relationships": {
                        "required": [
                          "contract"
                        ]
                      }
                    }
                  }
                ]
              }
            }
          }
        }
      }
    },
    "responses": {
      "204": {
        "description": "The shipment has been imported."
      }
    }
  }
}
