{
  "get": {
    "tags": [
      "Shipments"
    ],
    "summary": "Get a shipment by id.",
    "description": "This endpoint retrieves the shipment with the supplied id.",
    "parameters": [
      {
        "in": "path",
        "name": "shipment_id",
        "type": "string",
        "required": true,
        "description": "Id of the shipment."
      },
      {
        "in": "query",
        "name": "include",
        "type": "string",
        "description": "Comma separated string of the relationship names you want to include the data of.<br> The relationships that can be included are shop, service and status."
      }
    ],
    "responses": {
      "200": {
        "description": "Retrieved the shipment.",
        "schema": {
          "type": "object",
          "required": [
            "data"
          ],
          "properties": {
            "data": {
              "$ref": "#/definitions/Shipment"
            }
          }
        }
      }
    }
  },
  "delete": {
    "tags": [
      "Shipments"
    ],
    "summary": "Delete a shipment by id.",
    "description": "This endpoint deletes the shipment with the supplied id.",
    "parameters": [
      {
        "in": "path",
        "name": "shipment_id",
        "type": "string",
        "required": true,
        "description": "Id of the shipment."
      }
    ],
    "responses": {
      "202": {
        "description": "The shipment will be deleted."
      },
      "204": {
        "description": "The shipment is deleted."
      }
    }
  }
}
