{
  "get": {
    "tags": [
      "Files"
    ],
    "summary": "Get a file by id.",
    "description": "This endpoint retrieves the file with the supplied id.",
    "parameters": [
      {
        "in": "path",
        "name": "file_id",
        "type": "string",
        "required": true,
        "description": "Id of the file."
      },
      {
        "in": "header",
        "name": "Accept",
        "type": "string",
        "required": true,
        "enum": [
          "application/vnd.api+json",
          "application/pdf",
          "image/png",
          "image/jpeg"
        ],
        "description": "You can set the file header to either receive a json response, or the physical files in given format. Possible options are: <ul><li>`application/vnd.api+json`</li><li>`application/pdf`</li><li>`image/png`</li></ul>"
      }
    ],
    "produces": [
      "application/vnd.api+json",
      "application/pdf",
      "image/png"
    ],
    "responses": {
      "200": {
        "description": "Retrieved the file.",
        "schema": {
          "type": "object",
          "properties": {
            "data": {
              "$ref": "#/definitions/File"
            }
          }
        }
      }
    }
  }
}
