{
  "post": {
    "tags": [
      "RPC"
    ],
    "security": [
      {
        "OAuth2": [
          "shipments.manage"
        ]
      }
    ],
    "summary": "Import shipment.",
    "description": "This endpoint is used to import externally registered shipments to provide tracking information.",
    "requestBody": {
      "description": "The shipment that should be imported.",
      "required": true,
      "content": {
        "application/vnd.api+json": {
          "schema": {
            "type": "object",
            "required": [
              "data"
            ],
            "properties": {
              "data": {
                "required": [
                  "tracking_code"
                ],
                "allOf": [
                  {
                    "$ref": "#/components/schemas/Shipment"
                  },
                  {
                    "properties": {
                      "relationships": {
                        "required": [
                          "contract"
                        ]
                      }
                    }
                  }
                ],
                "properties": {
                  "tracking_code": {
                    "type": "string",
                    "description": "The tracking code of the shipment."
                  }
                }
              }
            }
          }
        }
      }
    },
    "responses": {
      "204": {
        "description": "The shipment has been imported."
      }
    }
  }
}
