{
  "type": "object",
  "required": [
    "type",
    "attributes"
  ],
  "properties": {
    "id": {
      "$ref": "../definitions/ReadOnlyUuid.json"
    },
    "type": {
      "type": "string",
      "example": "shipments"
    },
    "attributes": {
      "type": "object",
      "properties": {
        "recipient": {
          "$ref": "#/definitions/Address"
        },
        "sender": {
          "$ref": "#/definitions/Address"
        },
        "pickup": {
          "$ref": "#/definitions/Address"
        },
        "price": {
          "readOnly": true,
          "type": "number",
          "example": 10
        },
        "description": {
          "type": "string"
        },
        "currency": {
          "$ref": "../definitions/Currency.json"
        },
        "status": {
          "readOnly": true,
          "type": "string",
          "enum": [
            "Concept",
            "Pending/Registered",
            "Handed to carrier",
            "Sorting",
            "Distribution",
            "Delivered at customs",
            "Delivered",
            "Inactive",
            "Deleted",
            "Picked up"
          ]
        },
        "created": {
          "readOnly": true,
          "type": "string",
          "pattern": "[0-9]{4}-[0-9]{2}-[0-9]{2} [0-9]{2}:[0-9]{2}:[0-9]{2}",
          "example": "2015-02-14 17:12:48"
        },
        "modified": {
          "readOnly": true,
          "type": "string",
          "pattern": "[0-9]{4}-[0-9]{2}-[0-9]{2} [0-9]{2}:[0-9]{2}:[0-9]{2}",
          "example": "2015-02-14 17:12:48"
        },
        "insurance_amount": {
          "type": "number",
          "example": 10
        },
        "weight": {
          "type": "number",
          "example": 24
        },
        "status_last_synced": {
          "readOnly": true,
          "type": "string",
          "pattern": "[0-9]{4}-[0-9]{2}-[0-9]{2} [0-9]{2}:[0-9]{2}:[0-9]{2}",
          "example": "2015-02-14 17:12:48"
        },
        "options": {
          "type": "array",
          "items": {
            "$ref": "../definitions/ServiceOption.json"
          }
        },
        "customs_declaration": {
          "type": "array",
          "items": {
            "$ref": "../definitions/CustomsItem.json"
          }
        },
        "physical_properties": {
          "readOnly": true,
          "type": "object",
          "properties": {
            "height": {
              "type": "number",
              "example": 24
            },
            "width": {
              "type": "number",
              "example": 50
            },
            "length": {
              "type": "number",
              "example": 50
            },
            "volume": {
              "type": "number",
              "example": 50
            },
            "weight": {
              "type": "number",
              "example": 24
            },
          }
        }
      }
    },
    "relationships": {
      "type": "object",
      "properties": {
        "parent": {
          "readOnly": true,
          "type": "object",
          "properties": {
            "data": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "example": "shipments"
                },
                "id": {
                  "$ref": "../definitions/Uuid.json"
                }
              }
            },
            "links": {
              "readOnly": true,
              "type": "object",
              "properties": {
                "self": {
                  "type": "string",
                  "example": "/shipments/c9ce29a4-6325-11e7-907b-a6006ad3dba0"
                }
              }
            }
          }
        },
        "shop": {
          "type": "object",
          "properties": {
            "data": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "example": "shops"
                },
                "id": {
                  "$ref": "../definitions/Uuid.json"
                }
              }
            },
            "links": {
              "readOnly": true,
              "type": "object",
              "properties": {
                "self": {
                  "type": "string",
                  "example": "/shops/c9ce29a4-6325-11e7-907b-a6006ad3dba0"
                }
              }
            }
          }
        },
        "service": {
          "type": "object",
          "properties": {
            "data": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "example": "services"
                },
                "id": {
                  "$ref": "../definitions/Uuid.json"
                }
              }
            },
            "links": {
              "readOnly": true,
              "type": "object",
              "properties": {
                "self": {
                  "type": "string",
                  "example": "/carriers/c9ce29a4-6325-11e7-907b-a6006ad3dba0/services/c9ce29a4-6325-11e7-907b-a6006ad3dba0"
                }
              }
            }
          }
        }
      }
    },
    "links": {
      "readOnly": true,
      "type": "object",
      "properties": {
        "self": {
          "type": "string",
          "example": "/shipments/c9ce29a4-6325-11e7-907b-a6006ad3dba0"
        }
      }
    }
  }
}