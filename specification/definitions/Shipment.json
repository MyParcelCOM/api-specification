{
  "type": "object",
  "required": [
    "type",
    "attributes",
    "relationships"
  ],
  "properties": {
    "type": {
      "$ref": "../types/Shipment.json"
    },
    "id": {
      "$ref": "../includes/read-only-uuid.json"
    },
    "attributes": {
      "type": "object",
      "required": [
        "recipient_address",
        "weight"
      ],
      "additionalProperties": false,
      "properties": {
        "recipient_address": {
          "type": "object",
          "required": [
            "street_1",
            "city",
            "country_code"
          ],
          "additionalProperties": false,
          "properties": {
            "$ref": "../properties/Address.json"
          }
        },
        "sender_address": {
          "type": "object",
          "required": [
            "street_1",
            "city",
            "country_code"
          ],
          "additionalProperties": false,
          "properties": {
            "$ref": "../properties/Address.json"
          }
        },
        "pickup_location": {
          "type": "object",
          "properties": {
            "code": {
              "type": "string",
              "example": "123456"
            },
            "address": {
              "type": "object",
              "required": [
                "street_1",
                "city",
                "country_code"
              ],
              "additionalProperties": false,
              "properties": {
                "$ref": "../properties/Address.json"
              }
            }
          }
        },
        "description": {
          "type": "string",
          "example": "order #8008135"
        },
        "price": {
          "$ref": "../includes/read-only-price.json"
        },
        "insurance": {
          "$ref": "../includes/price.json"
        },
        "barcode": {
          "readOnly": true,
          "type": "string",
          "example": "3SABCD0123456789"
        },
        "tracking_code": {
          "readOnly": true,
          "type": "string",
          "example": "3SABCD0123456789"
        },
        "tracking_url": {
          "readOnly": true,
          "type": "string",
          "example": "https://tracker.carrier.com/3SABCD0123456789"
        },
        "weight": {
          "type": "number",
          "minimum": 0,
          "example": 24
        },
        "physical_properties": {
          "readOnly": true,
          "type": "object",
          "properties": {
            "height": {
              "type": "number",
              "minimum": 0,
              "example": 24
            },
            "width": {
              "type": "number",
              "minimum": 0,
              "example": 50
            },
            "length": {
              "type": "number",
              "minimum": 0,
              "example": 50
            },
            "volume": {
              "type": "number",
              "minimum": 0,
              "example": 50
            },
            "weight": {
              "type": "number",
              "minimum": 0,
              "example": 24,
            }
          }
        },
        "customs": {
          "type": "object",
          "required": [
            "content_type",
            "invoice_number",
            "items",
            "non_delivery",
            "incoterm"
          ],
          "additionalProperties": false,
          "properties": {
            "content_type": {
              "type": "string",
              "enum": [
                "merchandise",
                "sample_merchandise",
                "returned_merchandise",
                "documents",
                "gifts"
              ],
              "example": "merchandise"
            },
            "invoice_number": {
              "type": "string",
              "example": "9000"
            },
            "items": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "sku",
                  "description",
                  "item_value",
                  "quantity",
                  "hs_code",
                  "origin_country_code"
                ],
                "additionalProperties": false,
                "properties": {
                  "sku": {
                    "type": "string",
                    "minLength": 1,
                    "example": "123456789"
                  },
                  "description": {
                    "type": "string",
                    "minLength": 1,
                    "example": "OnePlus X"
                  },
                  "item_value": {
                    "$ref": "../includes/price.json"
                  },
                  "quantity": {
                    "type": "number",
                    "example": 2
                  },
                  "hs_code": {
                    "type": "string",
                    "pattern": "^[0-9.]+$",
                    "example": "8517.12.00"
                  },
                  "origin_country_code": {
                    "$ref": "../includes/country-code.json"
                  }
                }
              }
            },
            "non_delivery": {
              "type": "string",
              "enum": [
                "return",
                "abandon"
              ],
              "example": "return"
            },
            "incoterm": {
              "type": "string",
              "enum": [
                "DDU",
                "DDP"
              ],
              "example": "DDU"
            }
          }
        },
        "created_at": {
          "$ref": "../includes/read-only-timestamp.json"
        },
        "updated_at": {
          "$ref": "../includes/read-only-timestamp.json"
        },
        "synced_at": {
          "$ref": "../includes/read-only-timestamp.json"
        }
      }
    },
    "relationships": {
      "type": "object",
      "required": [
        "shop",
        "service",
        "contract"
      ],
      "properties": {
        "service_options": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "data": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "type",
                  "id"
                ],
                "additionalProperties": false,
                "properties": {
                  "type": {
                    "$ref": "../types/ServiceOption.json"
                  },
                  "id": {
                    "$ref": "../includes/uuid.json"
                  }
                }
              }
            }
          }
        },
        "parent": {
          "readOnly": true,
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "data": {
              "type": "object",
              "required": [
                "type",
                "id"
              ],
              "additionalProperties": false,
              "properties": {
                "type": {
                  "$ref": "../types/Shipment.json"
                },
                "id": {
                  "$ref": "../includes/uuid.json"
                }
              }
            },
            "links": {
              "readOnly": true,
              "type": "object",
              "properties": {
                "related": {
                  "type": "string",
                  "example": "$base_url/shipments/c9ce29a4-6325-11e7-907b-a6006ad3dba0"
                }
              }
            }
          }
        },
        "shop": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "data": {
              "type": "object",
              "required": [
                "type",
                "id"
              ],
              "additionalProperties": false,
              "properties": {
                "type": {
                  "$ref": "../types/Shop.json"
                },
                "id": {
                  "$ref": "../includes/uuid.json"
                }
              }
            },
            "links": {
              "readOnly": true,
              "type": "object",
              "properties": {
                "related": {
                  "type": "string",
                  "example": "$base_url/shops/c9ce29a4-6325-11e7-907b-a6006ad3dba0"
                }
              }
            }
          }
        },
        "status": {
          "readOnly": true,
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "data": {
              "type": "object",
              "required": [
                "type",
                "id"
              ],
              "additionalProperties": false,
              "properties": {
                "type": {
                  "$ref": "../types/ShipmentStatus.json"
                },
                "id": {
                  "$ref": "../includes/uuid.json"
                }
              }
            },
            "links": {
              "readOnly": true,
              "type": "object",
              "properties": {
                "related": {
                  "type": "string",
                  "example": "$base_url/shipments/c9ce29a4-6325-11e7-907b-a6006ad3dba0/statuses/c9ce29a4-6325-11e7-907b-a6006ad3dba0"
                }
              }
            }
          }
        },
        "service": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "data": {
              "type": "object",
              "required": [
                "type",
                "id"
              ],
              "additionalProperties": false,
              "properties": {
                "type": {
                  "$ref": "../types/Service.json"
                },
                "id": {
                  "$ref": "../includes/uuid.json"
                }
              }
            },
            "links": {
              "readOnly": true,
              "type": "object",
              "properties": {
                "related": {
                  "type": "string",
                  "example": "$base_url/services/c9ce29a4-6325-11e7-907b-a6006ad3dba0"
                }
              }
            }
          }
        },
        "contract": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "data": {
              "type": "object",
              "required": [
                "type",
                "id"
              ],
              "additionalProperties": false,
              "properties": {
                "type": {
                  "$ref": "../types/Contract.json"
                },
                "id": {
                  "$ref": "../includes/uuid.json"
                }
              }
            }
          }
        },
        "files": {
          "readOnly": true,
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "data": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "type",
                  "id"
                ],
                "additionalProperties": false,
                "properties": {
                  "type": {
                    "$ref": "../types/File.json"
                  },
                  "id": {
                    "$ref": "../includes/uuid.json"
                  }
                }
              }
            },
            "links": {
              "readOnly": true,
              "type": "object",
              "required": [
                "related"
              ],
              "properties": {
                "related": {
                  "type": "string",
                  "example": "$base_url/shipments/c9ce29a4-6325-11e7-907b-a6006ad3dba0/files"
                }
              }
            }
          }
        }
      }
    },
    "links": {
      "readOnly": true,
      "type": "object",
      "required": [
        "self"
      ],
      "properties": {
        "self": {
          "type": "string",
          "example": "$base_url/shipments/c9ce29a4-6325-11e7-907b-a6006ad3dba0"
        }
      }
    }
  }
}
