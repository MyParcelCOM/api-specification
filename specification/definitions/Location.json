{
  "type": "object",
  "required": [
    "id",
    "type",
    "attributes",
    "relationships"
  ],
  "properties": {
    "id": {
      "$ref": "../definitions/Uuid.json"
    },
    "type": {
      "type": "string",
      "example": "locations"
    },
    "attributes": {
      "type": "object",
      "properties": {
        "code": {
          "type": "string",
          "example": "176227"
        },
        "name": {
          "type": "string",
          "example": "MyParcel.com"
        },
        "address": {
          "$ref": "#/definitions/Address"
        },
        "phone_number": {
          "type": "string",
          "example": "+31 85 208 5997"
        },
        "opening_hours": {
          "type": "array",
          "items": {
            "$ref": "../definitions/OpeningHours.json"
          },
          "minItems": 7,
          "maxItems": 7
        },
        "position": {
          "$ref": "../definitions/Position.json"
        }
      }
    },
    "relationships": {
      "type": "object",
      "properties": {
        "carrier": {
          "type": "object",
          "properties": {
            "data":{
              "type": "object",
              "properties": {
                "id": {
                  "$ref": "../definitions/Uuid.json"
                },
                "type": {
                  "type": "string",
                  "example": "carriers"
                }
              }
            }
          }
        }
      }
    }
  }
}
