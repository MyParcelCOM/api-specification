          "type": "number",
          "example": 3000
        }
      }
    },
    "relationships": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "shop": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "data": {
              "type": "object",
              "required": [
                "type",
                "id"
              ],
              "additionalProperties": false,
              "properties": {
                "type": {
                  "$ref": "../types/Shop.json"
                },
                "id": {
                  "$ref": "../includes/uuid.json"
                }
              }
            },
            "links": {
              "readOnly": true,
              "type": "object",
              "properties": {
                "related": {
                  "type": "string",
                  "example": "$base_url/shops/c9ce29a4-6325-11e7-907b-a6006ad3dba0"
                }
              }
            }
          }
        },
        "region": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "data": {
              "type": "object",
              "required": [
                "type",
                "id"
              ],
              "additionalProperties": false,
              "properties": {
                "type": {
                  "$ref": "../types/Region.json"
                },
                "id": {
                  "$ref": "../includes/uuid.json"
                }
              }
            },
            "links": {
              "readOnly": true,
              "type": "object",
              "properties": {
                "related": {
                  "type": "string",
                  "example": "http://localhost:8080/v1/regions/c9ce29a4-6325-11e7-907b-a6006ad3dba0"
                }
              }
            }
          }
        }
      }
    }
