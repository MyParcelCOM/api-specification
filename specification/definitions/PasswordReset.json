{
  "type": "object",
  "required": [
    "type",
    "attributes"
  ],
  "additionalProperties": false,
  "properties": {
    "type": {
      "$ref": "../types/PasswordReset.json"
    },
    "id": {
      "$ref": "../includes/read-only-uuid.json"
    },
    "attributes": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "email": {
          "type": "string",
          "example": "john@doe.com"
        },
        "password": {
          "type": "string",
          "example": "secret"
        },
        "expires_at": {
          "$ref": "../includes/read-only-timestamp.json"
        }
      }
    }
  }
}
