{
  "type": "object",
  "required": [
    "id",
    "type",
    "attributes",
    "links"
  ],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$ref": "../definitions/ReadOnlyUuid.json"
    },
    "type": {
      "type": "string",
      "pattern": "^carriers$",
      "example": "carriers"
    },
    "attributes": {
      "type": "object",
      "required": [
        "name"
      ],
      "properties": {
        "name": {
          "type": "string",
          "example": "PostNL"
        }
      }
    },
    "links": {
      "type": "object",
      "readOnly": true,
      "properties": {
        "self": {
          "type": "string",
          "example": "$base_url/carriers/c9ce29a4-6325-11e7-907b-a6006ad3dba0"
        },
        "services": {
          "type": "string",
          "example": "$base_url/carriers/c9ce29a4-6325-11e7-907b-a6006ad3dba0/services"
        }
      }
    }
  }
}
