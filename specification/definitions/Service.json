{
  "type": "object",
  "required": [
    "type",
    "attributes",
    "relationships"
  ],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$ref": "../definitions/ReadOnlyUuid.json"
    },
    "type": {
      "type": "string",
      "pattern": "^services$",
      "example": "services"
    },
    "attributes": {
      "type": "object",
      "required": [
        "name",
        "package_type",
        "default_insurance"
      ],
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "string",
          "example": "Parcel to Parcelshop"
        },
        "package_type": {
          "type": "string",
          "example": "parcel"
        },
        "default_insurance": {
          "$ref": "../definitions/Price.json"
        }
      }
    },
    "relationships": {
      "type": "object",
      "properties": {
        "carrier": {
          "type": "object",
          "properties": {
            "links": {
              "type": "object",
              "properties": {
                "self": {
                   "type": "string",
                   "example": "$base_url/carriers/c9ce29a4-6325-11e7-907b-a6006ad3dba0"
                }
              }
            },
            "data":{
              "type": "object",
              "properties": {
                "id": {
                  "$ref": "../definitions/Uuid.json"
                },
                "type": {
                  "type": "string",
                  "example": "carriers"
                }
              }
            }
          }
        },
        "region_from": {
          "type": "object",
          "properties": {
            "links": {
              "type": "object",
              "properties": {
                "self": {
                   "type": "string",
                   "example": "$base_url/regions/c9ce29a4-6325-11e7-907b-a6006ad3dba0"
                }
              }
            },
            "data":{
              "type": "object",
              "properties": {
                "id": {
                  "$ref": "../definitions/Uuid.json"
                },
                "type": {
                  "type": "string",
                  "example": "regions"
                }
              }
            }
          }
        },
        "region_to": {
          "type": "object",
          "properties": {
            "links": {
              "type": "object",
              "properties": {
                "self": {
                  "type": "string",
                  "example": "$base_url/regions/c9ce29a4-6325-11e7-907b-a6006ad3dba0"
                }
              }
            },
            "data":{
              "type": "object",
              "properties": {
                "id": {
                  "$ref": "../definitions/Uuid.json"
                },
                "type": {
                  "type": "string",
                  "example": "regions"
                }
              }
            }
          }
        }
      }
    },
    "links": {
      "readOnly": true,
      "type": "object",
      "properties": {
        "self": {
          "type": "string",
          "example": "$base_url/services/c9ce29a4-6325-11e7-907b-a6006ad3dba0"
        },
        "prices": {
          "type": "string",
          "example": "$base_url/services/c9ce29a4-6325-11e7-907b-a6006ad3dba0/prices"
        }
      }
    }
  }
}
