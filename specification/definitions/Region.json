{
  "type": "object",
  "required": [
    "type",
    "attributes"
  ],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$ref": "../definitions/ReadOnlyUuid.json"
    },
    "type": {
      "type": "string",
      "pattern": "^regions$",
      "example": "regions"
    },
    "attributes": {
      "type": "object",
      "required": [
        "country_code",
        "currency",
        "name"
      ],
      "additionalProperties": false,
      "properties": {
        "country_code": {
          "type": "string",
          "example": "NL"
        },
        "region_code": {
          "type": "string",
          "example": "NH"
        },
        "currency": {
          "type": "string",
          "example": "EUR"
        },
        "name": {
          "type": "string",
          "example": "The Netherlands"
        },
        "parent_id": {
          "$ref": "../definitions/Uuid.json"
        }
      }
    }
  }
}
