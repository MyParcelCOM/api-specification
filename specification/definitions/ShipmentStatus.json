{
  "type": "object",
  "required": [
    "id",
    "type",
    "attributes"
  ],
  "properties": {
    "id": {
      "$ref": "../definitions/Uuid.json"
    },
    "type": {
      "type": "string",
      "example": "shipment_status"
    },
    "attributes": {
      "type": "object",
      "properties": {
        "status": {
          "type": "integer",
          "example": 3
        },
        "status_last_synced": {
          "type": "string",
          "example": "2017-07-13 17:30:00"
        },
        "status_description": {
          "type": "string",
          "example": "Handed to carrier"
        }
      }
    }
  }
}
