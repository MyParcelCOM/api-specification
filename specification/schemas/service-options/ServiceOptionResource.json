{
  "type": "object",
  "required": [
    "type"
  ],
  "properties": {
    "type": {
      "type": "string",
      "pattern": "^service-options$",
      "example": "service-options"
    },
    "id": {
      "type": "string",
      "format": "uuid",
      "pattern": "^[a-f\\d]{8}(-[a-f\\d]{4}){3}-[a-f\\d]{12}$",
      "example": "4c675b1a-516c-4410-abff-d237fd45bcd0"
    }
  }
}
