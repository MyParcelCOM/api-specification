{
  "allOf": [
    {
      "$ref": "#/components/schemas/ServiceOptionResource"
    },
    {
      "properties": {
        "attributes": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "name": {
              "type": "string",
              "maxLength": 255,
              "example": "Sunday Delivery"
            },
            "category": {
              "type": "string",
              "enum": [
                "proof-of-delivery",
                "handover-method",
                "delivery-window"
              ],
              "example": "delivery-window"
            },
            "code": {
              "type": "string",
              "maxLength": 255,
              "example": "delivery-window:sunday"
            }
          }
        }
      }
    }
  ]
}
