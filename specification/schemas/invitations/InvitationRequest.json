{
  "type": "object",
  "required": [
    "data"
  ],
  "additionalProperties": false,
  "properties": {
    "data": {
      "required": [
        "attributes",
        "relationships"
      ],
      "properties": {
        "id": {
          "readOnly": true
        },
        "attributes": {
          "required": [
            "email",
            "user_types"
          ],
          "properties": {
            "first_name": {
              "type": [
                "string",
                "null"
              ],
              "example": "Sherlock"
            },
            "last_name": {
              "type": [
                "string",
                "null"
              ],
              "example": "Holmes"
            },
            "email": {
              "$ref": "#/components/schemas/Email",
              "example": "s.holmes@holmesinvestigations.com"
            },
            "user_types": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": [
                  "shipping",
                  "returns"
                ]
              }
            },
            "locale": {
              "$ref": "#/components/schemas/LocaleEnum",
              "default": "en-GB",
              "description": "Specifies the language for the invitation email."
            }
          }
        },
        "relationships": {
          "type": "object",
          "required": [
            "broker"
          ],
          "additionalProperties": false,
          "properties": {
            "broker": {
              "$ref": "#/components/schemas/BrokerRelationship"
            }
          }
        }
      }
    }
  }
}
