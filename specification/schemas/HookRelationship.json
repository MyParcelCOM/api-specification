{
  "type": "object",
  "required": [
    "data"
  ],
  "additionalProperties": false,
  "properties": {
    "data": {
      "type": "object",
      "required": [
        "type",
        "id"
      ],
      "properties": {
        "type": {
          "type": "string",
          "pattern": "^hooks$",
          "example": "hooks"
        },
        "id": {
          "type": "string",
          "format": "uuid",
          "example": "99c2917a-8cbe-4e04-b4ce-e43263b20332"
        }
      }
    },
    "links": {
      "readOnly": true,
      "type": "object",
      "required": [
        "related"
      ],
      "additionalProperties": false,
      "properties": {
        "related": {
          "type": "string",
          "format": "url",
          "example": "$API_HOST/hooks/99c2917a-8cbe-4e04-b4ce-e43263b20332"
        }
      }
    }
  }
}
