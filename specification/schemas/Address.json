{
  "type": "object",
  "required": [
    "type",
    "attributes"
  ],
  "properties": {
    "type": {
      "type": "string",
      "pattern": "^addresses$",
      "example": "addresses"
    },
    "id": {
      "type": "integer",
      "format": "id",
      "example": "202790602",
      "readOnly": true
    },
    "attributes": {
      "$ref": "#/components/schemas/AddressAttributes"
    }
  }
}
