{
  "type": "object",
  "required": [
    "content_type",
    "non_delivery",
    "incoterm"
  ],
  "additionalProperties": false,
  "properties": {
    "content_type": {
      "type": "string",
      "enum": [
        "merchandise",
        "sample_merchandise",
        "returned_merchandise",
        "gifts",
        "documents"
      ],
      "example": "sample_merchandise"
    },
    "invoice_number": {
      "type": "string",
      "example": "9000"
    },
    "non_delivery": {
      "type": "string",
      "enum": [
        "return",
        "abandon"
      ],
      "example": "return",
      "description": "Action when the parcel cannot be delivered"
    },
    "incoterm": {
      "type": "string",
      "enum": [
        "DDU",
        "DDP"
      ],
      "example": "DDU",
      "description": "DDU (Delivered Duty Unpaid) or DDP (Delivered Duty Paid)"
    }
  }
}
