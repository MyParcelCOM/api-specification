{
  "type": "object",
  "allOf": [
    {
      "$ref": "#/components/schemas/MinimalCarrierContract"
    },
    {
      "required": [
        "relationships",
        "id"
      ],
      "properties": {
        "attributes": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "currency": {
              "$ref": "#/components/schemas/Currency"
            },
            "active": {
              "type": "boolean",
              "example": true
            },
            "name": {
              "type": "string",
              "example": "My Custom contract"
            },
            "credentials": {
              "type": "object",
              "example": {
                "$ref": "#/components/examples/CarrierContractCredentials"
              }
            }
          }
        },
        "relationships": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "carrier": {
              "$ref": "#/components/schemas/CarrierRelationship"
            },
            "service_contracts": {
              "$ref": "#/components/schemas/ServiceContractsRelationship"
            },
            "owner": {
              "oneOf": [
                {
                  "$ref": "#/components/schemas/BrokerRelationship"
                },
                {
                  "$ref": "#/components/schemas/OrganizationRelationship"
                }
              ]
            }
          }
        }
      }
    }
  ]
}
