{
  "allOf": [
    {
      "$ref": "#/components/schemas/BaseReport"
    },
    {
      "properties": {
        "attributes": {
          "type": "object",
          "additionalProperties": false,
          "required": [
            "name"
          ],
          "properties": {
            "name": {
              "type": "string",
              "example": "2020 Q1 UK shipment report"
            }
          }
        },
        "relationships": {
          "type": "object",
          "readOnly": true,
          "required": [
            "status"
          ],
          "additionalProperties": false,
          "properties": {
            "file": {
              "$ref": "#/components/schemas/FileRelationship"
            },
            "status": {
              "$ref": "#/components/schemas/StatusRelationship"
            }
          }
        }
      }
    }
  ]
}
