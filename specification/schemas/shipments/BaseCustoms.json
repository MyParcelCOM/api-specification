{
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "content_type": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "merchandise",
            "sample_merchandise",
            "returned_merchandise",
            "gifts",
            "documents"
          ]
        },
        {
          "type": "null"
        }
      ]
    },
    "invoice_number": {
      "type": [
        "null",
        "string"
      ],
      "maxLength": 255,
      "description": "Required when `content_type` is either `merchandise`, `sample_merchandise` or `returned_merchandise`.",
      "example": "9000"
    },
    "non_delivery": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "return",
            "abandon"
          ],
          "description": "Action when the parcel cannot be delivered."
        },
        {
          "type": "null"
        }
      ]
    },
    "incoterm": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "DAP",
            "DDP"
          ],
          "description": "DAP (Delivered At Place) or DDP (Delivered Duty Paid). Required when `content_type` is either `merchandise`, `sample_merchandise` or `returned_merchandise`."
        },
        {
          "type": "null"
        }
      ]
    },
    "shipping_value": {
      "oneOf": [
        {
          "$ref": "#/components/schemas/Price"
        },
        {
          "type": "null",
          "description": ""
        }
      ],
      "description": "The shipping cost paid by the customer, for tax and duty purposes."
    },
    "license_number": {
      "type": [
        "null",
        "string"
      ],
      "maxLength": 255,
      "example": "512842382"
    },
    "certificate_number": {
      "type": [
        "null",
        "string"
      ],
      "maxLength": 255,
      "example": "2112211"
    },
    "total_tax": {
      "readOnly": true,
      "oneOf": [
        {
          "$ref": "#/components/schemas/Price"
        },
        {
          "readOnly": true,
          "type": "null"
        }
      ]
    },
    "total_duty": {
      "readOnly": true,
      "oneOf": [
        {
          "$ref": "#/components/schemas/Price"
        },
        {
          "readOnly": true,
          "type": "null"
        }
      ]
    },
    "payment": {
      "readOnly": true,
      "oneOf": [
        {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "checkout_url": {
              "type": "string",
              "description": "Endpoint that generates a payment checkout URL.",
              "example": "https://api.myparcel.com/shipping/v2/shipments/{shipmentId}/payment-checkout-url/{paymentId}"
            },
            "paid_at": {
              "$ref": "#/components/schemas/Timestamp"
            },
            "price": {
              "$ref": "#/components/schemas/Price"
            }
          }
        },
        {
          "readOnly": true,
          "type": "null"
        }
      ]
    }
  }
}
